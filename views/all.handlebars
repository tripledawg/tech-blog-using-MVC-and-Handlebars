{{!-- for each to parse what is in blodpost object --}}
 {{#each blog_posts as |post|}}
 <div class="blogpost-card">
   <div class="dish col-md-5">
      <!-- -->
      <p>
        {{post.title}}
        {{post.user.username}}
        {{post.date_updated}}
        {{post.contents}}
      </p>
    </div>
  </div>
  {{/each}}